<h1>Listing lawngrounds</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lawngrounds.each do |lawnground| %>
      <tr>
        <td><%= lawnground.name %></td>
        <td><%= link_to 'Edit', edit_lawnground_path(lawnground) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
